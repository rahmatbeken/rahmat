<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#0000" />
            <stroke
                android:width="1dp"
                android:color="#ff42a115" />
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
            <corners android:radius="30dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ff42a115" />
            <padding
                android:left="10dp"
                android:right="10dp" />
            <corners android:radius="30dp" />
        </shape>
    </item>
</layer-list>